<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>{% block title %}Jetblack{% endblock %}</title>
        {% block stylesheets %}

            <!--Import Google Icon Font-->
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
            <!--Import materialize.css-->
            <link type="text/css" rel="stylesheet" href="{{asset('assets/css/materialize.css')}}"  media="screen,projection"/>
              <link type="text/css" rel="stylesheet" href="{{asset('assets/css/estilos.css')}}"  media="screen,projection"/>
        {% endblock %}
    </head>
    <body>

    

    {% block body %}
    
    
     <nav>
            <div class="nav-wrapper">
                <a href="{{ path('index')}}" class="brand-logo"><i class="large material-icons" id="hola">build</i>JetBlack</a>

                <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">

                 
                     <li><a href="{{ path('crear-producto')}}" class="waves-effect waves-light btn" >Productos </a></li>
                     <li><a href="{{ path('mis-productos')}}" class="waves-effect waves-light btn" id="boton">Ver productos </a></li>
                    <li><a href="{{ path('crear-categoria')}}" class="waves-effect waves-light btn">Categorias </a></li>
                    <li><a href="{{ path('mis-categorias')}}" class="waves-effect waves-light btn" id="boton">Ver categorias </a></li>
                </ul>
            </div>
        </nav>
        
        
        
     <ul class="sidenav" id="mobile-demo">

            <li><a href="" class="waves-effect waves-light btn">Productos </a></li>
            <li><a href="" class="waves-effect waves-light btn">Categorias </a></li>
        </ul>

        <br/>

        <div class="container">

            <div>
                <h2 class="center-align">Bienvenido al sistema</h2>

            </div>
            <br/>
        </div>

        
        
         <br/>

        <div class="container">

            <div>
                <h5 class="center-align">Productos con Stock</h5>

            </div>
            <br/>
        </div>



        <div class="container">


            <table class="responsive-table" id="tabla">
                <thead>
                    <tr>
                        <th>Nombre</th>

                        <th>Descripcion</th>
                        <th>Precio</th>
                        <th>Stock</th>
                    </tr>
                </thead>

                <tbody>
                   
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                    
                </tbody>
            </table>



        </div>

    
    
    
    
    
    {% endblock %}
    {% block javascripts %}
        <!--JavaScript at end of body for optimized loading-->
        <script type="text/javascript" src="{{asset('assets/js/materialize.min.js')}}"></script>
        
        <script>
document.addEventListener('DOMContentLoaded', function () {
    M.AutoInit();
});

    </script>

    {% endblock %}
</body>
</html>
